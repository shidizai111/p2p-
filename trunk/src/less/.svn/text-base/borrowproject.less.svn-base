// 公共样式
/* 浮动样式 */
.fl{
    float:left;
}
.fr{
    float:right;
}
/* 清除浮动 */
.clear{
    clear:both;
}
/* 清除固定结构浮动 */
.clearfix::after{
    content:"";
    width:0;
    height:0;
    display:block;
    clear:both;
}
