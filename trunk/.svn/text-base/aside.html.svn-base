<script>
    const index = location.hash;
    const panelList = $('.panel-collapse');
    $.each(panelList, (index, item)=>{
        // 清除激活样式
        $(item).children().children().removeClass('active');
        // 先全部收起
        $(item).removeClass('in');
    })
    // 展开对应
    const curLink = $(`#accordion a[href$=${index}]`);
    curLink.parent().parent().addClass('in');
    // 激活对应
    curLink.addClass('active')
</script>



